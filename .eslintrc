{
    "extends": "airbnb",
    "env": {
      "mocha": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "react/prop-types": [
          "off"
      ],
      "react/prefer-stateless-function": [
          "warn"
      ],
      "no-unused-expressions": 0
    }
}
